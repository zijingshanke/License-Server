<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:image field="logo" resource="logo.jpg"></ui:image>
	<ui:image field="logo_name" resource="logo_name.jpg"></ui:image>
	<ui:image field="help" resource="help.jpg"></ui:image>
	<ui:image field="helpover" resource="helpover.jpg"></ui:image>
	<ui:image field="helpdown" resource="helpdown.jpg"></ui:image>
	<ui:image field="log" resource="log.jpg"></ui:image>
	<ui:image field="logover" resource="logover.jpg"></ui:image>
	<ui:image field="logdown" resource="logdown.jpg"></ui:image>
	<ui:image field="exit" resource="exit.jpg"></ui:image>
	<ui:image field="exitover" resource="exitover.jpg"></ui:image>
	<ui:image field="exitdown" resource="exitdown.jpg"></ui:image>
	<ui:image field="title01" resource="title01.jpg"></ui:image>
	<ui:image field="title02" resource="title02.jpg" repeatStyle="Horizontal"></ui:image>
	<ui:image field="title03" resource="title03.jpg"></ui:image>
	<ui:image field="center" resource="center.jpg" repeatStyle="Vertical"></ui:image>
	<ui:image field="topbg" resource="topbg.jpg" repeatStyle="Horizontal"></ui:image>
	<ui:image field="top02" resource="top02.jpg" repeatStyle="Horizontal"></ui:image>
	<ui:image field="footer" resource="footer.jpg" repeatStyle="Horizontal"></ui:image>
	<!-- new -->
	<ui:image field="logo_new" resource="logo_new.png"></ui:image>

	<ui:style src="index.css"></ui:style>
	<ui:style src="new2.css" field="new2"></ui:style>
	<g:DockLayoutPanel ui:field="dock" unit='PX'
		styleName="{new2.dockbody}">
		<g:north size="83">
			<g:HTMLPanel>
				<!--head [[ -->
				<div class="{new2.index} {new2.head}" id="head">
					<div class="{new2.top-nav} {new2.clearfix}">
						<ul>
							<li class="{new2.on}">
								<g:Anchor text="管理中心" styleName="{new2.newa}" ui:field="managementCenter" />
							</li>
							<li>
								<g:Anchor text="员工天地" styleName="{new2.newa}" ui:field="staffCenter" />
							</li>
						</ul>
						<div class="{new2.green}">
							<g:InlineLabel ui:field="message"></g:InlineLabel>
							<g:Anchor ui:field="logBtn" styleName="{new2.newa}">安全退出</g:Anchor>
							<g:Anchor ui:field="collectionBtn" styleName="{new2.newa}">加为收藏
							</g:Anchor>
						</div>
					</div>
					<g:Image resource="{logo_new}" styleName="{new2.logo}"
						width="255px" height="75px" />
					<div class="{new2.nav} {new2.clearfix}">
						<ul>
							<li>
								<g:Anchor styleName="{new2.menu} {new2.menu-on}"
									ui:field="btnLicense">
									<span>授权管理</span>
								</g:Anchor>
							</li>
							<li>
								<g:Anchor styleName="{new2.menu}" ui:field="btnSetting">
									<span>设 置</span>
								</g:Anchor>
							</li>
							<li>
								<g:Anchor styleName="{new2.menu}" ui:field="btnStaff">
									<span>STAFF</span>
								</g:Anchor>
							</li>
						</ul>
					</div>
				</div>
				<!--head ]] -->
			</g:HTMLPanel>
		</g:north>
		<g:south size="60">
			<g:HTMLPanel>
				<div class="{new2.footer}" id="footer">
					<div class="{new2.footer-line}"></div>
					<div class="{new2.footer-wrap}">
						<div class="{new2.copyright}">
							<p>版权所有 深圳积享通信息技术有限公司 粤ICP备09007295</p>
							<p>电话：0755-82671268 传真：0755-86271610</p>
						</div>
					</div>
				</div>
			</g:HTMLPanel>
		</g:south>
		<g:west size="202">
			<g:HTMLPanel>
				<table width="100%" height="100%" border="0" cellspacing="0"
					cellpadding="0" id="container" style="margin-top:1px;background-color:#F4F4F4">
					<tr>
						<!-- menu -->
						<td width="250" valign="top" height="100%">
							<div class="{style.title}">
								<table width="100%" border="0" cellspacing="0"
									cellpadding="0">
									<tr>
										<!--<td> <g:Image resource="{title01}"></g:Image> </td> -->
										<td>
											<g:InlineLabel text="授权管理" ui:field="menuTitle" />
										</td>
										<!--<td id="title03"></td> -->
									</tr>
								</table>
							</div>
							<div class="{new2.list-top}"></div>
							<div id="menu">
								<!-- here insert menu widget -->
								<g:FlowPanel ui:field="menu">

								</g:FlowPanel>
							</div>
							<div class="{new2.list-bottom}"></div>
						</td>
						<td width="40" height="100%"
							style="background-image:none;background-color:#F4F4F4">
							<div id="center">
							</div>
						</td>
					</tr>
				</table>


			</g:HTMLPanel>
		</g:west>

	</g:DockLayoutPanel>
</ui:UiBinder>
